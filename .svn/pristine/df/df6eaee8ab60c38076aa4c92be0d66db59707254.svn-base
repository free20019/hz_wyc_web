@import "variable";

.tw-template {
  &-wrapper {
    height: calc(100% - 41px);
    width: 100%;
  }
  &-body {
    height: 100%;
    transition: width 0.5s ease, margin-left 0.5s ease;
  }
}

.tw-layout {
  $layout_height: 50px;
  $layout-left_width: 300px;
  position: relative;
  height: 100%;
  &-header,
  &-top {
    height: $layout_height;
    & + .tw-layout-left,
    & ~ .tw-layout-left {
      top: $layout_height;
      height: calc(100% - #{$layout_height});
    }
    & ~ .tw-layout-body {
      height: calc(100% - #{$layout_height});
    }
  }
  &-left {
    position: absolute;
    top: 0;
    left: 0;
    width: $layout-left_width;
    height: 100%;
    //border-right: 1px solid borderColor_default;
    box-sizing: border-box;
    transition: width 0.5s ease;
    & + .tw-layout-body {
      width: calc(100% - #{$layout-left_width});
      margin-left: $layout-left_width;
    }
  }
  &-body {
    height: 100%;
    transition: width 0.5s ease, margin-left 0.5s ease;
    overflow-y: hidden;
  }
  &-right {
    position: absolute;
    top: 0;
    right: 0;
    height: 100%;
    border-left: 1px solid $borderColor_default;
  }
}
.tw-index-tabs {
  .el-tabs__header {
    margin: 0;
  }
  &.el-tabs--top .el-tabs__item.is-top:nth-child(2) {
    padding-left: 20px;
  }
}
.tw-query {
  $height: 54px;
  &-bar {
    padding: $margin $margin 0 $margin;
    border-bottom: 1px solid $borderColor_default;
    .el-form-item {
      margin-bottom: $margin;
      &.el-form-item--mini,
      &.el-form-item--small {
        margin-bottom: $margin;
      }
    }
    & + .tw-template-body {
      height: calc(100% - #{$height});
    }
  }
  &-panel {
    height: calc(100% - #{$height});
    width: 100%;
    padding: $margin;
    box-sizing: border-box;
  }
}

.tw-map {
  &-box {
    height: 100%;
  }
  &-query-panel {
    position: absolute;
    top: 60px;
    left: 80px;
    width: 300px;
    height: calc(100% - 250px);
    min-height: 400px;
    padding: $margin;
    @include borderShadow;
    background-color: #ffffff;
  }
  &-tool {
    &-bar {
      position: absolute;
      line-height: 30px;
      @include borderShadow;
      background-color: #ffffff;
    }

    &-item {
      display: inline-block;
      margin: 0;
      padding: 0 $margin;
      user-select: none;

      &:first-child {
        border-top-left-radius: inherit;
        border-bottom-left-radius: inherit;
      }

      &:last-child {
        border-top-right-radius: inherit;
        border-bottom-right-radius: inherit;
      }

      &:hover,
      &.active {
        color: #ffffff;
        background-color: #0a5380;
      }
    }

    &-right {
      right: 0;
      width: calc(1em + 2 * #{$margin});
      border-top-right-radius: 0;
      border-bottom-right-radius: 0;
      line-height: 1.15;
    }

    &-right &-item {
      display: block;
      padding: $margin;

      &:first-child {
        border-top-right-radius: inherit;
        border-bottom-left-radius: 0;
      }

      &:last-child {
        border-top-right-radius: 0;
        border-bottom-left-radius: inherit;
      }
    }

    &-bottom {
      top: auto;
      left: 30px;
      right: auto;
      bottom: 0;
      border-bottom-left-radius: 0;
      border-bottom-right-radius: 0;
    }

    &-bottom &-item {
      &:hover {
        color: #000000;
        background-color: transparent;
      }

      &.el-form-item {
        margin-bottom: 0;
        &:last-child {
          margin-right: 0;
        }
      }
    }
  }
}

.tw-table {
  &.no-highlight {
    &.el-table--enable-row-hover .el-table__body tr:hover > td {
      background-color: #ffffff;
    }
  }
  tr {
    transition: height 0.5s;
    &:hover {
      .cell {
        white-space: normal;
      }
    }
    .cell {
      white-space: nowrap;
    }
  }
  width: 100%;
  margin-bottom: $margin;
  &:last-child {
    margin-bottom: 0;
  }
  .el-table__empty-block {
    width: 100% !important;
  }
}
.tw-page {
  &-bar {
    margin-bottom: $margin;
    &:last-child {
      margin-bottom: 0;
    }
    //&.dynamicMontor li {
    //  margin: 0;
    //  padding: 0px;
    //}
  }
}

.tw-chart {
  &-panel.echarts {
    overflow: hidden;
    width: 100%;
    height: 100%;
  }
}

.tw-tabs {
  height: calc(100% - 10px);

  .el-tab {
    &s__header {
      margin-bottom: 0;
    }

    &s__content {
      height: calc(100% - 40px);
    }

    &-pane {
      height: 100%;
      padding: $margin;
      box-sizing: border-box;
    }
  }
}

.tw-card {
  &-full {
    height: 100%;
    box-sizing: border-box;
    .el-card {
      &__header {
        + .el-card__body {
          height: calc(100% - 50px);
        }
      }
      &__body {
        overflow: auto;
        height: 100%;
        box-sizing: border-box;
      }
    }
  }
}

.tw-form {
  @mixin formItem($width) {
    .el-form-item {
      &__label {
        float: left;
        width: $width;
      }

      &__content {
        display: block;
        margin-left: $width;
      }
    }
  }
  @mixin form() {
    .el-form {
      &-item {
        &.tw-block {
          display: block;
          @include formItem(80px);

          &.tw-size__150px {
            @include formItem(150px);
          }
        }
        margin-bottom: $margin;
        &.el-form-item--mini,
        &.el-form-item--small {
          margin-bottom: $margin;
        }
      }
    }
  }
  @include form;
  padding: $margin $margin 0 $margin;
  border-bottom: 1px solid $borderColor_default;
}

.tw-btn {
  &-block {
    width: 100%;
    box-sizing: border-box;
  }
}

.tw-search-for {
  $fontColor: #ffffff;
  $backgroundColor: #409eff;
  $backgroundColor__hover: #66b1ff;

  .el-input-group__append {
    border-color: $backgroundColor;
    color: $fontColor;
    background-color: $backgroundColor;

    &:hover,
    &:focus {
      border-color: $backgroundColor__hover;
      color: $fontColor;
      background: $backgroundColor__hover;
    }
  }
  .el-input__suffix {
    position: absolute;
    right: 0;
    .el-button--small,
    .el-button--small.is-round {
      padding: 5px 25px 5px 15px;
      line-height: 1.5;
    }
    .el-input__icon {
      &:before {
      }
    }
  }
}

.tw-list {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  list-style: none;

  &-item {
    position: relative;
    line-height: 40px;
    border-bottom: 1px solid #eeeeee;
    cursor: pointer;

    &:last-child {
      border-bottom: none;
    }
    &:hover,
    &.active {
      background-color: #ccdffd;
    }
    &.title {
      background: none;
      cursor: auto;
    }
  }

  &-suffix {
    position: absolute;
    top: 50%;
    right: $margin;
    transform: translateY(-50%);
    &.el-button {
      padding: 7px;
      &:hover {
        text-shadow: 0 0 6px #000000;
      }
    }
  }

  &-border {
    border: 1px solid #eeeeee;
    border-radius: 4px;
    overflow: hidden;
  }
}

.tw-icon--prefix {
  margin-right: $margin;
}

.tw-login {
  &-title {
    position: absolute;
    top: calc(10% + 30px);
    left: 50%;
    font-size: 46px;
    text-shadow: 0 0 12px #55a4b3;
    white-space: nowrap;
    color: #12beeb;
    transform: translate(-50%, -50%);
  }
  &-box {
    position: relative;
    top: calc(50% - 50px);
    left: calc(50% - 300px - 30px);
    width: 300px;
    transform: translate(-50%, -50%);
  }
  &-item {
    margin-bottom: 20px;
    &:last-child {
      margin-bottom: 0;
    }
    .el-input__prefix {
      margin-left: 3px;
    }
  }
}

.tw-scrollbar {
  height: 100%;
  &.tw-y {
    .el-scrollbar__wrap {
      overflow-x: hidden;
    }
  }
  .el-scrollbar__view {
    //height: 100%;
  }
}

.tw-loading {
  height: 50px;
}

.tw-footer-bar {
  .el-form-item {
    height: 30px;
    margin-bottom: $margin;
    &__label {
      padding-right: 5px;
      line-height: 30px;

      &:last-child {
        margin-bottom: 0;
      }
    }
    &__content {
      line-height: 30px;
    }
  }
}

.tw-text {
  &-error {
    color: #e81123;
  }
}

/* ******过度动画****** */
