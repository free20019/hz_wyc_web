                    '#48a698',
                    '#479c7f',
                    '#48af54',
                    '#53b440',
                    '#67b52d',
                    '#8cc13f',
                    '#bbd337',
                    '#d2b130',
                    '#da9c2d',
                    '#dc9e31',
                    '#e59524',
                    '#e58b3d',
                    '#e7741b',
                    '#d15122',
                    '#ce4018',
                    '#d93824',
                    '#c71b1b',
                    '#bc1540',
                    '#a71a4f',
                    '#a71a4f',
                    '#a71a4f'
