//车辆动态监控左侧
.list-search {
  position: relative;
  height: 50px;
}
.list-input {
  position: absolute;
  left: 35px;
  &.el-input {
    position: absolute;
    width: 192px;
  }
}
.list-icon {
  position: absolute;
  left: 225px;
  width: 40px;
  height: 40px;
  font-size: 40px;
  color: #ffffff;
  background-color: #18e2ff;
  border-top-right-radius: 5px;
  border-bottom-right-radius: 5px;
  text-align: center;
  i {
    font-size: 25px;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
  }
}
//特定区域监测
.tw-map-left-panel {
  .el-collapse-item__header {
    padding-left: 60px;
    color: #374873;
    font-weight: 600;
    &.is-active {
      background-color: #c9e0f9;
    }
  }
  .el-collapse-item__arrow {
    margin: 0 75px 0 auto;
  }
}
// 车辆动态监控
.tw-tab {
  float: left;
}
.map-tool {
  float: left;
  width: 603px;
  .el-tabs__header {
    box-sizing: border-box;
    margin-bottom: 0;
    border: none;
    height: 50px;
    background-color: #ffffff;
  }
  .el-tabs__nav-next,
  .el-tabs__nav-prev {
    line-height: 52px;
    font-size: 26px;
    color: #000000;
  }
  .el-tabs__item {
    position: relative;
    text-align: center;
    padding: 0;
    height: 50px;
    width: 80px;
    box-sizing: border-box;
    color: inherit;
    &.is-active {
      background-color: #384a6e;
      color: #ffffff;
    }
  }
  &.el-tabs--card > .el-tabs__header .el-tabs__item.is-active {
    border: none;
  }
  &.el-tabs--card > .el-tabs__header .el-tabs__item {
    border-bottom: none;
  }
  &.el-tabs--card > .el-tabs__header {
    border-bottom: none;
  }
  &.el-tabs--card > .el-tabs__header .el-tabs__nav {
    border: none;
  }
  .el-tabs__nav-wrap {
    margin-bottom: -2px;
  }
}
.query-table {
  .move-line {
    position: absolute;
    top: -2px;
    width: 100%;
    height: 5px;
    cursor: n-resize;
  }
}
.tw-dialog-title {
  padding: 5px 5px 5px 20px;
  border-bottom: 1px solid #cccccc;
  position: relative;
  div {
    font-size: 16px;
    line-height: 16px;
    color: #3c96f0;
    font-weight: bold;
    &::before {
      content: "";
      position: absolute;
      left: 0;
      top: 4px;
      bottom: 4px;
      width: 4px;

      background: #3c96f0;
    }
  }
}
.tw-map-tool {
  border-radius: 8px;
  transition: all 0.5s;
  z-index: 50;
  &.show-tool {
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    + .tw-map-tool {
      border-top-left-radius: 0;
      border-bottom-left-radius: 0;
    }
  }
}
.tab-info {
  .el-tabs__item {
    padding: 0 15px;
  }
}
// 分割线
.tw-driver {
  // 垂直分割线
  &.vertical {
    background-color: #dcdfe6;
    display: inline-block;
    width: 1px;
    height: 1em;
    margin: 0 8px;
    vertical-align: middle;
    position: relative;
  }
}
// 特定区域监控，列表
.danger-list {
  color: red;
}
.el-table .danger-row {
  color: red;
}
.tw-template-height {
  height: 100%;
  .el-tabs__content {
    height: calc(100% - 56px);
    .el-tab-pane {
      height: 100%;
    }
  }
}
@mixin scrollbar() {
  width: 0px;
  height: 10px;
}
@mixin scrollbar-thumb() {
  background-color: #0e5997;
  border-radius: 3px;
}

.tw-map-left-panel {
  ::-webkit-scrollbar {
    @include scrollbar();
  }
  //滚动条的滑块
  ::-moz-scrollbar-thumb {
    @include scrollbar-thumb();
  }
  ::-moz-scrollbar {
    @include scrollbar();
  }
  //滚动条的滑块
  ::-webkit-scrollbar-thumb {
    @include scrollbar-thumb();
  }
  ::-m-scrollbar {
    @include scrollbar();
  }
  //滚动条的滑块
  ::-m-scrollbar-thumb {
    @include scrollbar-thumb();
  }
}

.cls-1 {
  fill: #c3d2f5;
  stroke: #95acea;
  stroke-miterlimit: 10;
}

