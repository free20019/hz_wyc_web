          left: '10%',
          right: '10%',
          bottom: '18%',
          top: '20%'
        },
        calculable: true,
        xAxis: [
          {
            type: 'category',
            boundaryGap: false,
            data: dateArr
          }
        ],
        yAxis: [
          {
            type: 'value',
            axisLabel: {
              formatter: '{value}'
            }
          }
        ],
        series: [
          {
            type: 'line',
            data: _.pluck(this.rmData, type),
            markPoint: {
              data: [{ type: 'max', name: '最大值' }, { type: 'min', name: '最小值' }]
            },
            markLine: {
              data: [{ type: 'average', name: '平均值' }]
            }
          }
          // {
          //   name: '最低气温',
          //   type: 'line',
          //   data: [1, -2, 2, 5, 3, 2, 0],
          //   markPoint: {
          //     data: [{ name: '周最低', value: -2, xAxis: 1, yAxis: -1.5 }]
          //   },
          //   markLine: {
          //     data: [{ type: 'average', name: '平均值' }]
          //   }
          // }
        ]
      })
    },
    // 预警数字接口
    getEarlyWarningNum() {
      axios.get('map/getEarlyWarningNum', { baseURL: this.baseURL }).then(res => {
        this.warning = res.data
      })
